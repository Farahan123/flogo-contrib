{
  "name": "tibco-couchbase",
  "type": "flogo:activity",
  "ref": "github.com/TIBCOSoftware/flogo-contrib/activity/couchbase",
  "version": "5.0.0.0",
  "title": "Upsert Couchbase document",
  "description": "Update or insert a Couchbase document in a specified bucket",
  "homepage": "https://github.com/TIBCOSoftware/flogo-contrib/tree/master/activity/couchbase",
  "inputs": [
    {
      "name": "key",
      "type": "string",
      "required": true
    },
    {
      "name": "data",
      "type": "string"
    },
    {
      "name": "method",
      "type": "string",
      "required": true,
      "allowed": [
        "Insert",
        "Upsert"
      ]
    },
    {
      "name": "expiry",
      "type": "integer",
      "value": 0,
      "required": true
    },
    {
      "name": "server",
      "type": "string",
      "required": true
    },
    {
      "name": "username",
      "type": "string"
    },
    {
      "name": "password",
      "type": "string"
    },
    {
      "name": "bucket",
      "type": "string",
      "required": true
    },
    {
      "name": "bucketPassword",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "name": "output",
      "type": "any"
    },
    {
      "name": "status",
      "type": "boolean"
    }
  ]
}
